{
  "mcpServers": {
    "webex-mcp-local": {
      "command": "go",
      "args": ["run", "."],
      "env": {
        "WEBEX_PUBLIC_WORKSPACE_API_KEY": "${WEBEX_PUBLIC_WORKSPACE_API_KEY}"
      }
    },
    "webex-mcp-docker": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env-file",
        ".env",
        "webex-mcp-server:latest"
      ]
    },
    "webex-mcp-built": {
      "command": "./build/webex-mcp-server",
      "env": {
        "WEBEX_PUBLIC_WORKSPACE_API_KEY": "${WEBEX_PUBLIC_WORKSPACE_API_KEY}"
      }
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    }
  }
}